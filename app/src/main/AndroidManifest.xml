<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="ufrpe.mobile.pickapp">

    <application
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity android:name="ufrpe.mobile.pickapp.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <activity
                android:name="ufrpe.mobile.pickapp.PeladaActivity"
                android:label="@string/title_activity_pelada"/>
        <activity android:name="ufrpe.mobile.pickapp.NovoUsuarioActivity">
        </activity>
    </application>

</manifest>